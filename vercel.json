{
  "version": 2,
  "builds": [
    {
      "src": "docs/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": ".vitepress/dist"
      }
    },
    {
      "src": "api/index.go",
      "use": "@vercel/go",
      "config": {
        "maxDuration": 10
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.go"
    },
    {
      "src": "/openapi\\.json$",
      "dest": "/api/index.go"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/$",
      "dest": "/docs/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/docs/$1"
    }
  ],
  "env": {
    "GO_VERSION": "1.23"
  }
}